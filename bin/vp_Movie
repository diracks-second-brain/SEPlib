#!/bin/csh -f
#$
#$=head1 NAME
#$
#$ vp_Movie - creates movie of vplot files
#$
#$=head1 SYNOPSIS
#$
#$ vp_Movie file1 [file2 file3 ...] > all.v
#$
#$=head1 DESCRIPTION
#$
#$ cat many vplot files to form a movie (run by xtpen)
#$
#$=head1 SEE ALSO:
#$
#$L<vp_OverUnderAniso>
#$
#$L<vp_OverUnderIso>
#$
#$L<vp_SideBySideAniso>
#$
#$L<vp_SideBySideIso>
#$
#$L<vp_Unrotate>
#$
#$L<vp_Overlay>
#$
#$=head1 CATEGORY
#$
#$ graphics/vplot/util/shells
#$
#$=head1 COMPILE LEVEL
#$
#$ DISTR
#$
#$=cut 
if ($#argv < 1)  then
   head -26 $0 |more	
	exit
else
# loop over command line
   set m = " "
   foreach i ($argv[1-$#argv])
           set m = ($m $i)
      end
   vppen $m vpstyle=n
endif
