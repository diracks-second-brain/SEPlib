#!/bin/sh
#  Keyword:  LMO canon
if test -t 0
  then
#
echo "
#$
#$=head1 NAME
#$
#$LMO - perform LMO
#$
#$=head1 DESCRIPTION                                                              
#$Canonical Lmo , linear interp. and tabulation. Invoking Stretch rule=lmo  
#$
#$=head1 SYNOPSIS
#$                                                           
#$LMO conj=0 inv=0 [vel=1.5] [velfile=vel.H] < in.H > out.H    
#$
#$=head1 SEE ALSO
#$
#$L<Stretch>, L<Nmo3d>
#$
#$=head1 CATEGORY
#$
#$B<seis/velan>
#$
#$=cut
"
#
  sleep 2
  Stretch
else
  echo Stretch rule=lmo2 $*
       Stretch rule=lmo2 $*
fi
