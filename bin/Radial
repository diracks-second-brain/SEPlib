#!/bin/sh
#  Keyword:  Radial canon
if test -t 0
  then
#
  echo "
#$
#$=head1 NAME
#$
#$Radial - radial trace without nmo
#$
#$=head1 DESCRIPTION
#$
#$Radial trace without nmo.	Invoking Stretch rule=radial
#$
#$=head1 SYNOPSIS
#$
#$Radial conj=0 inv=0 [vel=1.5] [velfile=vel.H] < in.H > out.H
#$
#$=head1 SEE ALSO
#$
#$L<Stretch>
#$
#$=head1 CATEGORY
#$
#$B<seis/velan>
#$
#$=cut
#$
"
#
  sleep 2
  Stretch
else
  echo Stretch rule=radial $*
       Stretch rule=radial $*
fi
